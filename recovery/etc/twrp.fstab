# mount point	fstype		device			[device2]
/boot		emmc	/dev/block/mmcblk0p21
/recovery	emmc	/dev/block/mmcblk0p22
/cache		ext4	/dev/block/mmcblk0p25
/data		ext4	/dev/block/mmcblk0p24
/system		ext4	/dev/block/mmcblk0p23
/devlog		emmc	/dev/block/mmcblk0p29
/misc		emmc	/dev/block/mmcblk0p35
/sdcard		vfat	/dev/block/mmcblk1p1	/dev/block/mmcblk1	flags=display="Micro SDcard";storage;removable;wipeingui
/usb-otg	vfat	/dev/block/sda1			/dev/block/sda		flags=display="USB Memory";storage;removable
